          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "",
          "legendFormat": "{{node}}",
          "refId": "C"
        },
        {
          "exemplar": false,
          "expr": "avg_over_time (headroom_cpu_optimal_point_utilization{datacenter=~\"$Datacenter\",cluster=~\"$Cluster\"} [$__range])",
